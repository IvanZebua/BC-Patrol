<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".SOS_POS1GMO_CPP">

    <TextView
        android:id="@+id/tv_ListPatroliCPP"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="List Patroli CPP"
        android:textColor="@color/green_berau"
        android:textSize="40sp"
        android:textStyle="bold"
        android:gravity="center"/>

    <TextView
        android:id="@+id/tv_ListPatroliCPP_1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Room RC 1 dan RC 2"
        android:textSize="18sp"
        android:textStyle="bold"
        android:textColor="@color/green_berau"
        android:layout_marginStart="20dp"
        android:layout_marginTop="20dp"
        android:layout_marginEnd="20dp"
        app:layout_constraintTop_toBottomOf="@+id/tv_ListPatroliCPP"/>
    <EditText
        android:id="@+id/et_ListPatroliCPP_1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:hint="Keterangan"
        android:textColor="@color/black"
        android:textSize="14sp"
        android:layout_marginStart="20dp"
        android:layout_marginEnd="20dp"
        app:layout_constraintTop_toBottomOf="@+id/tv_ListPatroliCPP_1"/>
    <Button
        android:id="@+id/btn_ListPatroliCPP_1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Sudah Patroli"
        android:layout_marginStart="20dp"
        android:layout_marginEnd="20dp"
        app:layout_constraintTop_toBottomOf="@+id/et_ListPatroliCPP_1"/>

    <TextView
        android:id="@+id/tv_ListPatroliCPP_2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Hopper"
        android:textSize="18sp"
        android:textStyle="bold"
        android:textColor="@color/green_berau"
        android:layout_marginStart="20dp"
        android:layout_marginTop="20dp"
        android:layout_marginEnd="20dp"
        app:layout_constraintTop_toBottomOf="@+id/btn_ListPatroliCPP_1"/>
    <EditText
        android:id="@+id/et_ListPatroliCPP_2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:hint="Keterangan"
        android:textColor="@color/black"
        android:textSize="14sp"
        android:layout_marginStart="20dp"
        android:layout_marginEnd="20dp"
        app:layout_constraintTop_toBottomOf="@+id/tv_ListPatroliCPP_2"/>
    <Button
        android:id="@+id/btn_ListPatroliCPP_2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Sudah Patroli"
        android:layout_marginStart="20dp"
        android:layout_marginEnd="20dp"
        app:layout_constraintTop_toBottomOf="@+id/et_ListPatroliCPP_2"/>

    <TextView
        android:id="@+id/tv_ListPatroliCPP_3"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Stok File"
        android:textSize="18sp"
        android:textStyle="bold"
        android:textColor="@color/green_berau"
        android:layout_marginStart="20dp"
        android:layout_marginTop="20dp"
        android:layout_marginEnd="20dp"
        app:layout_constraintTop_toBottomOf="@+id/btn_ListPatroliCPP_2"/>
    <EditText
        android:id="@+id/et_ListPatroliCPP_3"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:hint="Keterangan"
        android:textColor="@color/black"
        android:textSize="14sp"
        android:layout_marginStart="20dp"
        android:layout_marginEnd="20dp"
        app:layout_constraintTop_toBottomOf="@+id/tv_ListPatroliCPP_3"/>
    <Button
        android:id="@+id/btn_ListPatroliCPP_3"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Sudah Patroli"
        android:layout_marginStart="20dp"
        android:layout_marginEnd="20dp"
        app:layout_constraintTop_toBottomOf="@+id/et_ListPatroliCPP_3"/>

    <TextView
        android:id="@+id/tv_ListPatroliCPP_4"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Crusser"
        android:textSize="18sp"
        android:textStyle="bold"
        android:textColor="@color/green_berau"
        android:layout_marginStart="20dp"
        android:layout_marginTop="20dp"
        android:layout_marginEnd="20dp"
        app:layout_constraintTop_toBottomOf="@+id/btn_ListPatroliCPP_3"/>
    <EditText
        android:id="@+id/et_ListPatroliCPP_4"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:hint="Keterangan"
        android:textColor="@color/black"
        android:textSize="14sp"
        android:layout_marginStart="20dp"
        android:layout_marginEnd="20dp"
        app:layout_constraintTop_toBottomOf="@+id/tv_ListPatroliCPP_4"/>
    <Button
        android:id="@+id/btn_ListPatroliCPP_4"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Sudah Patroli"
        android:layout_marginStart="20dp"
        android:layout_marginEnd="20dp"
        app:layout_constraintTop_toBottomOf="@+id/et_ListPatroliCPP_4"/>

</androidx.constraintlayout.widget.ConstraintLayout>