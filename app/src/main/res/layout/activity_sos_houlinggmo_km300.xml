<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".SOS_HOULINGGMO_KM300">

    <TextView
        android:id="@+id/tv_ListPatroliKM300"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="List Patroli KM 3.00"
        android:textColor="@color/green_berau"
        android:textSize="40sp"
        android:textStyle="bold"
        android:gravity="center"
        app:layout_editor_absoluteY="60dp"/>

    <Button
        android:id="@+id/btn_SOS_HOULINGGMO_KM300_1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Genangan Air Dumblok"
        android:textSize="16sp"
        android:textColor="@color/white"
        android:background="@color/green_berau"
        android:layout_marginTop="20dp"
        android:layout_marginStart="100dp"
        android:layout_marginEnd="100dp"
        app:layout_constraintTop_toBottomOf="@+id/tv_ListPatroliKM300"/>

    <Button
        android:id="@+id/btn_SOS_HOULINGGMO_KM300_2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Genangan Air Seam F"
        android:textSize="16sp"
        android:textColor="@color/white"
        android:background="@color/green_berau"
        android:layout_marginTop="20dp"
        android:layout_marginStart="100dp"
        android:layout_marginEnd="100dp"
        app:layout_constraintTop_toBottomOf="@+id/btn_SOS_HOULINGGMO_KM300_1"/>

    <Button
        android:id="@+id/btn_SOS_HOULINGGMO_KM300_3"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Genangan Air Seam G"
        android:textSize="16sp"
        android:textColor="@color/white"
        android:background="@color/green_berau"
        android:layout_marginTop="20dp"
        android:layout_marginStart="100dp"
        android:layout_marginEnd="100dp"
        app:layout_constraintTop_toBottomOf="@+id/btn_SOS_HOULINGGMO_KM300_2"/>

    <Button
        android:id="@+id/btn_SOS_HOULINGGMO_KM300_4"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Genangan Air Seam H"
        android:textSize="16sp"
        android:textColor="@color/white"
        android:background="@color/green_berau"
        android:layout_marginTop="20dp"
        android:layout_marginStart="100dp"
        android:layout_marginEnd="100dp"
        app:layout_constraintTop_toBottomOf="@+id/btn_SOS_HOULINGGMO_KM300_3"/>

</androidx.constraintlayout.widget.ConstraintLayout>