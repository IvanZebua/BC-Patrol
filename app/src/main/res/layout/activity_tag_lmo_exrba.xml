<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".TAG_LMO_EXRBA">

    <TextView
        android:id="@+id/tv_TAG_LMO_ER"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Sasaran Patroli EX RBA"
        android:textColor="@color/green_berau"
        android:textSize="40sp"
        android:textStyle="bold"
        android:gravity="center"/>

    <TextView
        android:id="@+id/tv_TAG_LMO_ER_1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Genset"
        android:textSize="18sp"
        android:textStyle="bold"
        android:textColor="@color/green_berau"
        android:layout_marginStart="20dp"
        android:layout_marginTop="20dp"
        android:layout_marginEnd="20dp"
        app:layout_constraintTop_toBottomOf="@+id/tv_TAG_LMO_ER"/>
    <EditText
        android:id="@+id/et_TAG_LMO_ER_1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:hint="Keterangan"
        android:textColor="@color/black"
        android:textSize="14sp"
        android:layout_marginStart="20dp"
        android:layout_marginEnd="20dp"
        app:layout_constraintTop_toBottomOf="@+id/tv_TAG_LMO_ER_1"/>
    <LinearLayout
        android:id="@+id/ll_TAG_LMO_ER_1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="20dp"
        android:layout_marginEnd="20dp"
        android:gravity="center_vertical"
        android:orientation="horizontal"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/et_TAG_LMO_ER_1">

        <TextView
            android:id="@+id/tv_TAG_LMO_ER_1_time"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:hint="19/5/2022 08:10"
            android:padding="5dp"
            android:textSize="14sp" />

        <Button
            android:id="@+id/btn_TAG_LMO_ER_1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="20dp"
            android:layout_marginEnd="20dp"
            android:text="Sudah Patroli" />
    </LinearLayout>

    <TextView
        android:id="@+id/tv_TAG_LMO_ER_2"
