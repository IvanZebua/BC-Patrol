<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".SOS_POS1GMO_BeachingPoint">

    <TextView
        android:id="@+id/tv_SOS_BMO1KM28_BP"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="List Beaching Point"
        android:textColor="@color/green_berau"
        android:textSize="40sp"
        android:textStyle="bold"
        android:gravity="center"/>

    <TextView
        android:id="@+id/tv_SOS_BMO1KM28_BP_1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Rambu OBVITNAS"
        android:textSize="18sp"
        android:textStyle="bold"
        android:textColor="@color/green_berau"
        android:layout_marginStart="20dp"
        android:layout_marginTop="20dp"
        android:layout_marginEnd="20dp"
        app:layout_constraintTop_toBottomOf="@+id/tv_SOS_BMO1KM28_BP"/>
    <EditText
        android:id="@+id/et_SOS_BMO1KM28_BP_1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:hint="Keterangan"
        android:textColor="@color/black"
        android:textSize="14sp"
        android:layout_marginStart="20dp"
        android:layout_marginEnd="20dp"
        app:layout_constraintTop_toBottomOf="@+id/tv_SOS_BMO1KM28_BP_1"/>
    <Button
        android:id="@+id/btn_SOS_BMO1KM28_BP_1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Sudah Patroli"
        android:layout_marginStart="20dp"
        android:layout_marginEnd="20dp"
        app:layout_constraintTop_toBottomOf="@+id/et_SOS_BMO1KM28_BP_1"/>

    <TextView
        android:id="@+id/tv_SOS_BMO1KM28_BP_2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Kapal/Perahu"
        android:textSize="18sp"
        android:textStyle="bold"
        android:textColor="@color/green_berau"
        android:layout_marginStart="20dp"
        android:layout_marginTop="20dp"
        android:layout_marginEnd="20dp"
        app:layout_constraintTop_toBottomOf="@+id/btn_SOS_BMO1KM28_BP_1"/>
    <EditText
        android:id="@+id/et_SOS_BMO1KM28_BP_2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:hint="Keterangan"
        android:textColor="@color/black"
        android:textSize="14sp"
        android:layout_marginStart="20dp"
        android:layout_marginEnd="20dp"
        app:layout_constraintTop_toBottomOf="@+id/tv_SOS_BMO1KM28_BP_2"/>
    <Button
        android:id="@+id/btn_SOS_BMO1KM28_BP_2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Sudah Patroli"
        android:layout_marginStart="20dp"
        android:layout_marginEnd="20dp"
        app:layout_constraintTop_toBottomOf="@+id/et_SOS_BMO1KM28_BP_2"/>

</androidx.constraintlayout.widget.ConstraintLayout>