<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".SOS_HaulingGMO">

    <TextView
        android:id="@+id/SOS_HOULINGGMO_ListMenu"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="List Menu Patroli Houling GMO "
        android:textSize="30sp"
        android:textStyle="bold"
        android:textColor="@color/green_berau"
        android:gravity="center"
        app:layout_editor_absoluteY="60dp"/>

    <Button
        android:id="@+id/btn_SOS_HOULINGGMO_1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="KM 0.400"
        android:textSize="16sp"
        android:textColor="@color/white"
        android:background="@color/green_berau"
        android:layout_marginTop="20dp"
        android:layout_marginStart="100dp"
        android:layout_marginEnd="100dp"
        app:layout_constraintTop_toBottomOf="@+id/SOS_HOULINGGMO_ListMenu"/>

    <Button
        android:id="@+id/btn_SOS_HOULINGGMO_2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="KM 1.800"
        android:textSize="16sp"
        android:textColor="@color/white"
        android:background="@color/green_berau"
        android:layout_marginTop="20dp"
        android:layout_marginStart="100dp"
        android:layout_marginEnd="100dp"
        app:layout_constraintTop_toBottomOf="@+id/btn_SOS_HOULINGGMO_1"/>

    <Button
        android:id="@+id/btn_SOS_HOULINGGMO_3"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="KM 3.00"
        android:textSize="16sp"
        android:textColor="@color/white"
        android:background="@color/green_berau"
        android:layout_marginTop="20dp"
        android:layout_marginStart="100dp"
        android:layout_marginEnd="100dp"
        app:layout_constraintTop_toBottomOf="@+id/btn_SOS_HOULINGGMO_2"/>

</androidx.constraintlayout.widget.ConstraintLayout>