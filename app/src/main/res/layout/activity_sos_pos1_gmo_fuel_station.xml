<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".SOS_POS1GMO_FuelStation">

    <TextView
        android:id="@+id/tv_SOS_BMO1KM28_FS"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="List Fuel Station"
        android:textColor="@color/green_berau"
        android:textSize="40sp"
        android:textStyle="bold"
        android:gravity="center"/>

    <TextView
        android:id="@+id/tv_SOS_BMO1KM28_FS_1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Tanki BBM"
        android:textSize="18sp"
        android:textStyle="bold"
        android:textColor="@color/green_berau"
        android:layout_marginStart="20dp"
        android:layout_marginTop="20dp"
        android:layout_marginEnd="20dp"
        app:layout_constraintTop_toBottomOf="@+id/tv_SOS_BMO1KM28_FS"/>
    <EditText
        android:id="@+id/et_SOS_BMO1KM28_FS_1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:hint="Keterangan"
        android:textColor="@color/black"
        android:textSize="14sp"
        android:layout_marginStart="20dp"
        android:layout_marginEnd="20dp"
        app:layout_constraintTop_toBottomOf="@+id/tv_SOS_BMO1KM28_FS_1"/>
    <Button
        android:id="@+id/btn_SOS_BMO1KM28_FS_1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Sudah Patroli"
        android:layout_marginStart="20dp"
        android:layout_marginEnd="20dp"
        app:layout_constraintTop_toBottomOf="@+id/et_SOS_BMO1KM28_FS_1"/>

    <TextView
        android:id="@+id/tv_SOS_BMO1KM28_FS_2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Pagar Pembatas"
        android:textSize="18sp"
        android:textStyle="bold"
        android:textColor="@color/green_berau"
        android:layout_marginStart="20dp"
        android:layout_marginTop="20dp"
        android:layout_marginEnd="20dp"
        app:layout_constraintTop_toBottomOf="@+id/btn_SOS_BMO1KM28_FS_1"/>
    <EditText
        android:id="@+id/et_SOS_BMO1KM28_FS_2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:hint="Keterangan"
        android:textColor="@color/black"
        android:textSize="14sp"
        android:layout_marginStart="20dp"
        android:layout_marginEnd="20dp"
        app:layout_constraintTop_toBottomOf="@+id/tv_SOS_BMO1KM28_FS_2"/>
    <Button
        android:id="@+id/btn_SOS_BMO1KM28_FS_2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Sudah Patroli"
        android:layout_marginStart="20dp"
        android:layout_marginEnd="20dp"
        app:layout_constraintTop_toBottomOf="@+id/et_SOS_BMO1KM28_FS_2"/>

    <TextView
        android:id="@+id/tv_SOS_BMO1KM28_FS_3"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Rambu Larangan"
        android:textSize="18sp"
        android:textStyle="bold"
        android:textColor="@color/green_berau"
        android:layout_marginStart="20dp"
        android:layout_marginTop="20dp"
        android:layout_marginEnd="20dp"
        app:layout_constraintTop_toBottomOf="@+id/btn_SOS_BMO1KM28_FS_2"/>
    <EditText
        android:id="@+id/et_SOS_BMO1KM28_FS_3"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:hint="Keterangan"
        android:textColor="@color/black"
        android:textSize="14sp"
        android:layout_marginStart="20dp"
        android:layout_marginEnd="20dp"
        app:layout_constraintTop_toBottomOf="@+id/tv_SOS_BMO1KM28_FS_3"/>
    <Button
        android:id="@+id/btn_SOS_BMO1KM28_FS_3"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Sudah Patroli"
        android:layout_marginStart="20dp"
        android:layout_marginEnd="20dp"
        app:layout_constraintTop_toBottomOf="@+id/et_SOS_BMO1KM28_FS_3"/>

</androidx.constraintlayout.widget.ConstraintLayout>