<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".SOS_HOULINGGMO_KM300_GenanganAirSeamF">

    <TextView
        android:id="@+id/tv_ListPatroliKM1800_GASF"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="List Patroli Genangan Seam F"
        android:textColor="@color/green_berau"
        android:textSize="40sp"
        android:textStyle="bold"
        android:gravity="center"/>

    <TextView
        android:id="@+id/tv_ListPatroliKM1800_GASF_1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Pompa Multiflow MTL"
        android:textSize="18sp"
        android:textStyle="bold"
        android:textColor="@color/green_berau"
        android:layout_marginStart="20dp"
        android:layout_marginTop="20dp"
        android:layout_marginEnd="20dp"
        app:layout_constraintTop_toBottomOf="@+id/tv_ListPatroliKM1800_GASF"/>
    <EditText
        android:id="@+id/et_ListPatroliKM1800_GASF_1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:hint="Keterangan"
        android:textColor="@color/black"
        android:textSize="14sp"
        android:layout_marginStart="20dp"
        android:layout_marginEnd="20dp"
        app:layout_constraintTop_toBottomOf="@+id/tv_ListPatroliKM1800_GASF_1"/>
    <Button
        android:id="@+id/btn_ListPatroliKM1800_GASF_1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Sudah Patroli"
        android:layout_marginStart="20dp"
        android:layout_marginEnd="20dp"
        app:layout_constraintTop_toBottomOf="@+id/et_ListPatroliKM1800_GASF_1"/>

    <TextView
        android:id="@+id/tv_ListPatroliKM1800_GASF_2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Void"
        android:textSize="18sp"
        android:textStyle="bold"
        android:textColor="@color/green_berau"
        android:layout_marginStart="20dp"
        android:layout_marginTop="20dp"
        android:layout_marginEnd="20dp"
        app:layout_constraintTop_toBottomOf="@+id/btn_ListPatroliKM1800_GASF_1"/>
    <EditText
        android:id="@+id/et_ListPatroliKM1800_GASF_2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:hint="Keterangan"
        android:textColor="@color/black"
        android:textSize="14sp"
        android:layout_marginStart="20dp"
        android:layout_marginEnd="20dp"
        app:layout_constraintTop_toBottomOf="@+id/tv_ListPatroliKM1800_GASF_2"/>
    <Button
        android:id="@+id/btn_ListPatroliKM1800_GASF_2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Sudah Patroli"
        android:layout_marginStart="20dp"
        android:layout_marginEnd="20dp"
        app:layout_constraintTop_toBottomOf="@+id/et_ListPatroliKM1800_GASF_2"/>

    <TextView
        android:id="@+id/tv_ListPatroliKM1800_GASF_3"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Rambu Larangan"
        android:textSize="18sp"
        android:textStyle="bold"
        android:textColor="@color/green_berau"
        android:layout_marginStart="20dp"
        android:layout_marginTop="20dp"
        android:layout_marginEnd="20dp"
        app:layout_constraintTop_toBottomOf="@+id/btn_ListPatroliKM1800_GASF_2"/>
    <EditText
        android:id="@+id/et_ListPatroliKM1800_GASF_3"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:hint="Keterangan"
        android:textColor="@color/black"
        android:textSize="14sp"
        android:layout_marginStart="20dp"
        android:layout_marginEnd="20dp"
        app:layout_constraintTop_toBottomOf="@+id/tv_ListPatroliKM1800_GASF_3"/>
    <Button
        android:id="@+id/btn_ListPatroliKM1800_GASF_3"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Sudah Patroli"
        android:layout_marginStart="20dp"
        android:layout_marginEnd="20dp"
        app:layout_constraintTop_toBottomOf="@+id/et_ListPatroliKM1800_GASF_3"/>
</androidx.constraintlayout.widget.ConstraintLayout>