<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".SOS_HOULINGGMO_KM0400">

    <TextView
        android:id="@+id/tv_ListPatroliKM0400"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="List Patroli KM 0.400"
        android:textColor="@color/green_berau"
        android:textSize="40sp"
        android:textStyle="bold"
        android:gravity="center"
        app:layout_editor_absoluteY="60dp"/>

    <Button
        android:id="@+id/btn_SOS_HOULINGGMO_KM0400_Timbangan"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Timbangan"
        android:textSize="16sp"
        android:textColor="@color/white"
        android:background="@color/green_berau"
        android:layout_marginTop="20dp"
        android:layout_marginStart="100dp"
        android:layout_marginEnd="100dp"
        app:layout_constraintTop_toBottomOf="@+id/tv_ListPatroliKM0400"/>

    <Button
        android:id="@+id/btn_SOS_HOULINGGMO_KM0400_JH"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Jalan Houling"
        android:textSize="16sp"
        android:textColor="@color/white"
        android:background="@color/green_berau"
        android:layout_marginTop="20dp"
        android:layout_marginStart="100dp"
        android:layout_marginEnd="100dp"
        app:layout_constraintTop_toBottomOf="@+id/btn_SOS_HOULINGGMO_KM0400_Timbangan"/>

</androidx.constraintlayout.widget.ConstraintLayout>